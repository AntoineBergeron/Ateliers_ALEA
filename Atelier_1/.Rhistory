IC <- function(donnees1, donnees2){
donnees <- donnees2 - donnees1
moy <- mean(donnees)
sd <- sd(donnees)
t <- 2.365
me <- t * sd/sqrt(8)
ICfinal <- c(moy-me,moy+me)
y <- c(moy-me,moy,moy+me)
return(y)
}
