val <- function(age,val_cont,poste,nb_but,nb_passe,min_moy){
val_act <- (-0.05*(age - 30)^2 + 7.5) + (val_cont + 5/100*val_cont) + poste + (nb_but*0.75 + nb_passe*0.55 + min_moy/30)
return(val_act)
}
